[
  {
    "context": "Workspace",
    "bindings": {
      "shift shift": "file_finder::Toggle"
    }
  },
  {
    "context": "vim_mode == insert",
    "bindings": {
      "j k": "vim::NormalBefore"
    }
  },
  {
    "context": "vim_mode == normal && !menu",
    "bindings": {
      "ctrl-j": "vim::PageDown",
      "ctrl-k": "vim::PageUp",
      "tab": "pane::ActivateNextItem",
      "shift-tab": "pane::ActivatePrevItem",
      "+ space": ["workspace::SendKeystrokes", "escape"]
    }
  }
]
